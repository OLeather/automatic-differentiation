    "from autodiff import *\n",
    "\n",
    "x = Var(3)\n",
    "y = Var(2)\n",
    "z = Var(5)\n",
    "\n",
    "# y = 2x + x^5 + cos(x^2)\n",
    "# y' = 2 + 5x^4 - sin(x^2)*2x\n",
    "\n",
    "w1 = plus(plus(mul(y, x), pow(x,z)), cos(pow(x, y)))\n",
    "\n",
    "print(w1.v, w1.d(x))"
   ]
